{
    // Required
    "app": {
      "background": {
        // Optional
        "scripts": ["background.js"]
      }
    },
    "manifest_version": 2,
    "name": "My App",
    "version": "versionString",
  
    // Recommended
    "default_locale": "en",
    "description": "A plain text description",
    "icons": { "16": "icon16.png",
        "48": "icon48.png",
       "128": "icon128.png" },
  
    "externally_connectable": {
        "matches": ["*://*.example.com/*"]
      }
  }