{
  "manifest_version": 2,
  "name": "Star Wars Weather",
  "description": "Sets theme to start wars planet matching weather in your location",
  "version": "1.0.0",
  "icons": {
    "16": "icon16.png",
    "48": "icon48.png",
    "128": "icon128.png"
  },
  "browser_action": {
    "default_popup": "popup.html"
  },
  "content_scripts": [
    {
      "matches": ["<all_urls>"],
      "js": ["contentScript.js"]
    }
  ],
  "permissions": [
    "activeTab"
  ]
}